# Example environment variables. Do NOT commit real secrets.
PORT=5000

# Postgres connection URL for Neon or other provider
# Example: postgresql://user:password@host:5432/dbname?sslmode=require
DATABASE_URL=

# Optional: local-only variables
PGHOST=
PGDATABASE=
PGUSER=
PGPASSWORD=
PGPORT=5432

# Secret used to protect the DB initialization endpoint. Set this in Vercel and locally.
INIT_SECRET=some-random-secret
